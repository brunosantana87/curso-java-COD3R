package fundamentos;

public class NotacaoPonto {

	public static void main(String[] args) {
		
		String s = "Bom dia X";
		// s.replace("X", "Senhora");
		s = s.replace("X", "Senhora");
		
		// s.toUpperCase();
		// fazendo só isso não altera o conteúdo da String
		s = s.toUpperCase();
		
		System.out.println(s);
	}
}
